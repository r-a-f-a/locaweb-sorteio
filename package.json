{
	"name": "sorteio",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"serve": "vue-cli-service serve",
		"build": "vue-cli-service build",
		"lint": "vue-cli-service lint",
		"test:e2e": "vue-cli-service test:e2e",
		"server": "yarn api && node server.js",
		"api": "npx json-server db.json &",
		"start": "yarn api && yarn serve",
		"sync": "node sync/index.js"
	},
	"dependencies": {
		"axios": "^0.19.0",
		"core-js": "^3.3.2",
		"eslint-config-standard": "^14.1.0",
		"eslint-plugin-import": "^2.18.2",
		"eslint-plugin-node": "^10.0.0",
		"eslint-plugin-promise": "^4.2.1",
		"eslint-plugin-standard": "^4.0.1",
		"events-vue-allin": "^1.3.0",
		"json-server": "^0.15.1",
		"node-xlsx": "^0.15.0",
		"register-service-worker": "^1.6.2",
		"shuffle-array": "^1.0.1",
		"vue": "^2.6.10",
		"vue-meta": "^2.3.1",
		"vue-router": "^3.1.3",
		"vue2-animate": "^2.1.3",
		"vuex": "^3.0.1"
	},
	"devDependencies": {
		"@vue/cli-plugin-babel": "^4.0.0",
		"@vue/cli-plugin-e2e-cypress": "^4.0.0",
		"@vue/cli-plugin-eslint": "^4.0.0",
		"@vue/cli-plugin-pwa": "^4.0.0",
		"@vue/cli-service": "^4.0.0",
		"@vue/eslint-config-prettier": "^5.0.0",
		"babel-eslint": "^10.0.3",
		"eslint": "^5.16.0",
		"eslint-plugin-prettier": "^3.1.1",
		"eslint-plugin-vue": "^5.0.0",
		"node-sass": "^4.13.1",
		"prettier": "^1.18.2",
		"sass-loader": "^8.0.0",
		"vue-template-compiler": "^2.6.10"
	},
	"eslintConfig": {
		"root": true,
		"parserOptions": {
			"parser": "babel-eslint"
		},
		"extends": [
			"plugin:vue/essential",
			"standard"
		],
		"plugins": [
			"vue"
		],
		"rules": {
			"generator-star-spacing": "off",
			"no-debugger": "error"
		}
	},
	"postcss": {
		"plugins": {
			"autoprefixer": {}
		}
	},
	"browserslist": [
		"> 1%",
		"last 2 versions"
	]
}
